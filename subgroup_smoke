
data aging_pool;
   set out.ds_aging_pool_same_agebase70;

   if bmi_avg>=25 then bmi_2cat=1;
   else bmi_2cat=0;

   if age86>=60 then age_2cat=1;
   else age_2cat=0;

   if alco_avg=0 then alco_zero=0;
   else alco_zero=1;

   if alco_avg>14 then alco_2cat=1; /* this may end up being men? */
   else alco_2cat=0;

   /* never smokers vs others */

   if smk=1 then smk_2cat=0; /* smk=1 is never smoker */
   else smk_2cat=1;
run;

proc means data=aging_pool n nmiss median;
   var pa_avg alco_avg;
run;


data pa_below;
   set aging_pool; 
   if smk_2cat=1 then delete; /* deleting smokers */
run;


data pa_above;
   set aging_pool; 
   if smk_2cat=0 then delete;
run;


/* Logistic Regression Model by sex - macro */
%macro logist_below(var1,dep,mvout);

   proc logistic data=pa_below;
      class race_new(ref="1") pmh(ref="1") ;
      model &dep(event='1')=&var1 sex age86 /clparm=wald ;
     
      ods output ParameterEstimates=pe2;
      ods output  OddsRatios=or2;
   run;

   ODS OUTPUT CLOSE;
   
   data or2;
      set or2;
      RR=put(OddsRatioEst,4.2)|| ' (' ||put(LowerCL,4.2)|| ', ' ||put(UpperCL,4.2)|| ')';
      newid=_N_;
      keep  newid RR;
   run;
   
   data pe2;
      set pe2;
      newid=_N_ -1;
      keep newid Variable ProbChiSq;
   run;

   data &mvout;
      merge pe2 or2;
      by newid;
      keep newid Variable RR ProbChiSq;
   run;

%mend logist_below;

%macro logist_above(var1,dep,mvout);

   proc logistic data=pa_above;
      class race_new(ref="1") pmh(ref="1") ;
      model &dep(event='1')=&var1 sex age86 /clparm=wald ;
     
      ods output ParameterEstimates=pe2;
      ods output  OddsRatios=or2;
   run;

   ODS OUTPUT CLOSE;
   
   data or2;
      set or2;
      RR=put(OddsRatioEst,4.2)|| ' (' ||put(LowerCL,4.2)|| ', ' ||put(UpperCL,4.2)|| ')';
      newid=_N_;
      keep  newid RR;
   run;
   
   data pe2;
      set pe2;
      newid=_N_ -1;
      keep newid Variable ProbChiSq;
   run;

   data &mvout;
      merge pe2 or2;
      by newid;
      keep newid Variable RR ProbChiSq;
   run;

%mend logist_above;


/* pa_below */
%logist_below(var1=ahei_avg_as race_new alone_ever marry_base nSES_86 bmi_avg mvever pmh    pa_avg
depres famdem hx_can hx_db hx_cvd cafh mifh dbfh, dep=healthy_ager_full, mvout=ahei_cont_results_MV);

 %logist_below(var1=amed_avg_as race_new alone_ever  marry_base nSES_86 bmi_avg mvever pmh    pa_avg
